{
  "name": "package.json",
  "description": "Airbnb reserve box",
  "author": "Luke",
  "license": "no copies",
  "engines": {
    "node": ">=6.13.0"
  },
  "scripts": {
    "build": "webpack --mode production",
    "babel-build": "babel client/src --watch -d lib",
    "start": "nodemon server/server.js",
    "schemaCreateSeed": "mysql -u root -p < schema.sql",
    "jsExecuteSeed": "node seed.js",
    "seed": "npm run schemaCreateSeed && npm run jsExecuteSeed"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@webpack-cli/init": "^0.3.0",
    "babel-loader": "^8.1.0",
    "eslint-config-hackreactor": "git://github.com/reactorcore/eslint-config-hackreactor",
    "nodemon": "^2.0.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
